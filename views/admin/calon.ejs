<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>NykVote</title>
    <link rel="stylesheet" href="../vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Aleo">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Asap">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Bitter:400,700">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Delius">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Ubuntu+Condensed">
    <link rel="shortcut icon" href="../images/favicon.ico">
    <link rel="stylesheet" href="../stylesheets/simple-sidebar.css">
</head>
<body onload="ready()">
    <div class="d-flex" id="wrapper">
        <% include ./sidebar %>
        <div id="page-content-wrapper">
            <% include ./navbar %>
            <div class="container-fluid">
                <h1 class="mt-4">NykVote</h1>
                <div class="container mb-3 mt-5">
                    <table class="table table-dark table-bordered" width="100%" id="table_id"  style="margin-top: 20px;">
                        <thead>
                            <tr>
                                <th>No</th>
                                <th>Nama Calon</th>
                                <th>Parpol / Organisasi</th>
                                <th>Lokasi Foto Profil</th>
                                <th>Event ID</th>
                                <th>Jumlah Vote</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i=0; i < calon_name.length; i++) { %>
                            <tr>
                                <td>
                                    <%= i+1 %>
                                </td>
                                <td>
                                    <%= calon_name[i] %>
                                </td>
                                <td>
                                    <%= parpol[i] %>
                                </td>
                                <td>
                                    <%= image[i] %>
                                </td>
                                <td>
                                    <%= event_id[i] %>
                                </td>
                                <td>
                                    <%= jum_vote[i] %>
                                </td>
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <script src="../vendor/jquery/jquery.min.js"></script>
    <script src="../vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
    <script>
        $("#menu-toggle").click(function(e) {
          e.preventDefault();
          $("#wrapper").toggleClass("toggled");
        });
        function ready(){
            document.getElementById('menu-toggle').click();
            $('#table_id').DataTable();
        }
    </script>
</body>
</html>